{
  "hooks": {
    "SessionStart": [
      {
        "matcher": "*",
        "hooks": [
          {
            "type": "command",
            "command": "node \".claude/plugins/code-quality/scripts/hooks/session-start.js\""
          }
        ],
        "description": "Check for SESSION.md and previous context on new session"
      }
    ],
    "SessionEnd": [
      {
        "matcher": "*",
        "hooks": [
          {
            "type": "command",
            "command": "node \".claude/plugins/code-quality/scripts/hooks/session-end.js\""
          }
        ],
        "description": "Persist session state on end"
      }
    ],
    "PreCompact": [
      {
        "matcher": "*",
        "hooks": [
          {
            "type": "command",
            "command": "node \".claude/plugins/code-quality/scripts/hooks/pre-compact.js\""
          }
        ],
        "description": "Save state before context compaction"
      }
    ],
    "PreToolUse": [
      {
        "matcher": "tool == \"Bash\" && tool_input.command matches \"git push\"",
        "hooks": [
          {
            "type": "command",
            "command": "node \".claude/plugins/code-quality/scripts/hooks/pre-push-check.js\""
          }
        ],
        "description": "Reminder to run tests before pushing"
      }
    ],
    "PostToolUse": [
      {
        "matcher": "tool == \"Edit\" && tool_input.file_path matches \"\\\\.py$\"",
        "hooks": [
          {
            "type": "command",
            "command": "node \".claude/plugins/code-quality/scripts/hooks/check-python-prints.js\""
          }
        ],
        "description": "Warn about print() statements in Python files"
      }
    ]
  },
